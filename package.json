{
  "name": "joints-supperui",
  "version": "0.1.0",
  "description": "ui designed for admin",
  "main": "./lib",
  "scripts": {
    "start": "gulp default",
    "gulp-build-default": "gulp build-default",
    "clean": "rimraf ./lib && rimraf ./dest",
    "build": "npm run clean && npm run gulp-build-default && babel ./src --out-dir ./lib",
    "test": "mocha --compilers js:babel-core/register --require ./test/support/entry.js --recursive",
    "test:watch": "npm run test -- --watch --watch-extensions js,jsx"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pistech/joints-supperui.git"
  },
  "keywords": [
    "joints",
    "react",
    "components",
    "supperui"
  ],
  "author": "ray liu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pistech/joints-supperui/issues"
  },
  "homepage": "https://github.com/pistech/joints-supperui#readme",
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.2",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.2.0",
    "browser-sync": "^2.11.0",
    "browserify": "^13.0.0",
    "chai": "^3.5.0",
    "chai-immutable": "^1.5.4",
    "del": "^2.2.0",
    "gulp": "^3.8.10",
    "gulp-clean-css": "^2.0.7",
    "gulp-compass": "^2.1.0",
    "gulp-nodemon": "^2.0.6",
    "gulp-notify": "^2.1.0",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^2.2.0",
    "gulp-strip-debug": "^1.1.0",
    "gulp-uglify": "^1.5.3",
    "gulp-util": "^3.0.1",
    "mocha": "^2.4.5",
    "require-dir": "^0.3.0",
    "rimraf": "^2.5.2",
    "underscore": "^1.7.0",
    "vinyl-buffer": "^1.0.0",
    "vinyl-paths": "^2.1.0",
    "vinyl-source-stream": "^1.0.0",
    "watchify": "^3.6.1"
  },
  "dependencies": {
    "react": "^0.13.3",
    "classnames": "^2.2.3",
    "pubsub-js": "^1.5.2"
  }
}
